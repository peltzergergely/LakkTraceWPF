<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
        <add name="CCTrace.CCDBConnectionString"
            connectionString="Host=localhost;Database=CCDB;Username=postgres;Password=admin;Persist Security Info=True"
            providerName="Npgsql" />
    </connectionStrings>
  <appSettings>
    <add key="productTxBx" value="^.{4}$"/>
  <!--  final regex for the carrier
        ^(BMW|VOLVO)\/OBC\/(TOP|BOT)\/\d{4}\/\d{2}$ -->  
  <add key="carrierTxbx" value="^(BMW|VOLVO).*(BOT|TOP)$"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
</configuration>